{# jinja2 template for rendering CSW 2.0.2 GetCapabilities requests #}
<?xml version="1.0" encoding="UTF-8"?>
<Capabilities
        xmlns="http://www.opengis.net/cat/csw/2.0.2"
        {% for prefix, identifier in namespaces.items() %}
        xmlns:{{ prefix }}="{{ identifier }}"
        {% endfor %}
        updateSequence="{{ updateSequence }}"
        version="{{ version }}">
</Capabilities>
